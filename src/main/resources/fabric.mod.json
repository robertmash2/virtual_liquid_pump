	"authors": [
		"rmash"
	],
	"contact": {
		"homepage": "https://github.com/robertmash2/mod_fabric_hello_world",
		"sources": "https://github.com/robertmash2/mod_fabric_hello_world"
	},
	"license": "CC0-1.0",
